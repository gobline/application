{
    "name": "mendoframework/mvc",
    "type": "library",
    "description": "Mendo MVC Framework",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Mathieu Decaffmeyer",
            "email": "mdecaffmeyer@gmail.com"
        }
    ],
    "keywords": [
        "mf", "mendoframework", "mendo framework",
        "mvc", "mvc framework",
        "hmvc", "hmvc framework",
        "mvvm", "mvvm framework"
    ],
    "homepage": "https://github.com/mendoframework",
    "autoload": {
        "psr-4": {
            "Mendo\\Mvc\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "pimple/pimple": "3.0.*@dev",
        "mendoframework/acl": "dev-master",
        "mendoframework/auth": "dev-master",
        "mendoframework/filter": "dev-master",
        "mendoframework/flash": "dev-master",
        "mendoframework/form": "dev-master",
        "mendoframework/http": "dev-master",
        "mendoframework/mediator": "dev-master",
        "mendoframework/registrar": "dev-master",
        "mendoframework/router": "dev-master",
        "mendoframework/session": "dev-master",
        "mendoframework/translator": "dev-master",
        "zendframework/zend-escaper": "~2.3",
        "filp/whoops": "1.2.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*@dev",
        "fabpot/php-cs-fixer": "*@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8.x-dev"
        }
    }
}